{
  "name": "microservice-generator",
  "version": "0.0.2",
  "description": "An opinionated generator for Swagger based Rest API servers",
  "author": "Dave Sag <davesag@gmail.com>",
  "type": "commonjs",
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/davesag"
  },
  "main": "src/index.js",
  "bin": {
    "ms-new": "node src/"
  },
  "files": [
    "src",
    "CONTRIBUTING.md"
  ],
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": ">= 12.18.2"
  },
  "scripts": {
    "eslint-check": "eslint --print-config src/index.js | eslint-config-prettier-check",
    "lint": "eslint .",
    "prettier": "prettier --write '**/*.{js,json,md}'",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test NODE_PATH=. mocha ./test/unit/ --require ./test/unitTestHelper.js --recursive",
    "test:unit:cov": "NODE_ENV=test NODE_PATH=. nyc mocha ./test/unit/ --require ./test/unitTestHelper.js --recursive",
    "test:mutants": "NODE_ENV=test NODE_PATH=. npx stryker run",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davesag/microservice-generator.git"
  },
  "bugs": {
    "url": "https://github.com/davesag/microservice-generator/issues"
  },
  "homepage": "https://github.com/davesag/microservice-generator#readme",
  "dependencies": {
    "commander": "^6.1.0",
    "rimraf": "^3.0.2",
    "slugify": "^1.4.5",
    "traverse-folders": "^1.1.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^3.3.1",
    "@stryker-mutator/javascript-mutator": "^3.3.1",
    "@stryker-mutator/mocha-framework": "^3.3.1",
    "@stryker-mutator/mocha-runner": "^3.3.1",
    "ajv": "^6.12.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "snyk": "^1.401.0"
  },
  "keywords": [
    "ampq",
    "api",
    "cli",
    "docker",
    "generator",
    "postgres",
    "rabbitmq",
    "rest",
    "sequelize",
    "swagger",
    "templates"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "never",
    "arrowParens": "avoid",
    "trailingComma": "none",
    "printWidth": 100
  },
  "lint-staged": {
    "**/*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "cache": true
  },
  "snyk": true
}
